<!--HOTEL STOCK PHOTO -->
<div class="index-picture">

    <div id="imageCarouseltop" class="carousel slide carousel-fade" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#imageCarouseltop" data-slide-to="0" class="active"></li>
            <li data-target="#imageCarouseltop" data-slide-to="1"></li>
            <li data-target="#imageCarouseltop" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="carousel-item active">
                <div class="view">
                    <img class="d-block w-100 mx-auto" src="img/indexhotel1.jpg" alt="First index slide">
                    <div class="mask rgba-black-slight"></div>
                </div>
            </div>
            <div class="carousel-item">
                <div class="view">
                    <img class="d-block w-100 mx-auto" src="img/indexhotel2.jpg" alt="Second index slide">
                    <div class="mask rgba-black-slight"></div>
                </div>
            </div>
            <div class="carousel-item">
                <div class="view">
                    <img class="d-block w-100 mx-auto" src="img/indexhotel3.jpg" alt="Third index slide">
                    <div class="mask rgba-black-slight"></div>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#imageCarouseltop" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#imageCarouseltop" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <!--        <img class="stockhotel img-fluid" src="img/indexhotel1.jpg">-->
</div>

